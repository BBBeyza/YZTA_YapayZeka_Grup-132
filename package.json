{
  "name": "project-starter",
  "version": "1.0.0",
  "description": "Neurograph project with Flutter frontend and Python backend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k \"cd frontend && flutter run\" \"cd backend/python && uvicorn api:app --host 0.0.0.0 --port 8000 --reload\"",
    "backend": "cd backend/python && uvicorn api:app --host 192.168.1.160 --port 8000 --reload",
    "backend-local": "cd backend/python && uvicorn api:app --host 192.168.1.160 --port 8000 --reload",
    "backend-all": "cd backend/python && uvicorn api:app --host 192.168.1.160 --port 8000 --reload",
    "frontend": "cd frontend && flutter run",
    "dev": "concurrently -k -n \"FLUTTER,BACKEND\" -c \"blue,green\" \"npm run frontend\" \"npm run backend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "flutter",
    "python",
    "neurograph",
    "tremor",
    "analysis"
  ],
  "author": "YZTA-Grup-132",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {}
}